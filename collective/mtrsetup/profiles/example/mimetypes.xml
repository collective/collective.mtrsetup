<?xml version="1.0"?>
<object name="mimetypes_registry" meta_type="MimeTypes Registry">


  <!-- OPENOFFICE MIMETYPES -->
  <!-- mimetypes: http://framework.openoffice.org/documentation/mimetypes/mimetypes.html -->
  <!-- icons: http://ui.openoffice.org/VisualDesign/OOo30MimeType.html -->

  <mimetype name="OpenDocument Text"
            mimetypes="application/vnd.oasis.opendocument.text
                       application/vnd.sun.xml.writer
                       application/vnd.stardivision.writer
                       application/x-starwriter"
            extensions="odt sxw sdw"
            globs="*.odt *.sxw *.sdw"
            icon_path="/++resource++mtr-icons/odt.png"
            binary="True"
            />

  <mimetype name="OpenDocument Text Template"
            mimetypes="application/vnd.oasis.opendocument.text-template
                       application/vnd.sun.xml.writer.template"
            extensions="ott stw"
            globs="*.ott *stw"
            icon_path="/++resource++mtr-icons/ott.png"
            binary="True"
            />

  <mimetype name="HTML Document Template"
            mimetypes="application/vnd.oasis.opendocument.text-web"
            extensions="oth"
            globs="*.oth"
            icon_path="/++resource++mtr-icons/oth.png"
            binary="True"
            />

  <mimetype name="OpenDocument Master Document"
            mimetypes="application/vnd.oasis.opendocument.text-master"
            extensions="odm"
            globs="*.odm"
            icon_path="/++resource++mtr-icons/oo-empty.png"
            binary="True"
            />

  <mimetype name="OpenDocument Drawing"
            mimetypes="application/vnd.oasis.opendocument.graphics
                       application/vnd.sun.xml.draw
                       application/vnd.stardivision.draw
                       application/x-stardraw"
            extensions="odg sxd sda"
            globs="*.odg *.sxd *.sda"
            icon_path="/++resource++mtr-icons/odg.png"
            binary="True"
            />

  <mimetype name="OpenDocument Drawing Template"
            mimetypes="application/vnd.oasis.opendocument.graphics-template
                       application/vnd.sun.xml.draw.template"
            extensions="otg std"
            globs="*.otg *.std"
            icon_path="/++resource++mtr-icons/otg.png"
            binary="True"
            />

  <mimetype name="OpenDocument Presentation"
            mimetypes="application/vnd.oasis.opendocument.presentation
                       application/vnd.sun.xml.impress
                       application/vnd.stardivision.impress
                       application/x-starimpress"
            extensions="odp sxi sdd"
            globs="*.odp *.sxi *.sdd"
            icon_path="/++resource++mtr-icons/odp.png"
            binary="True"
            />

  <mimetype name="OpenDocument Presentation Template"
            mimetypes="application/vnd.oasis.opendocument.presentation-template
                       application/vnd.sun.xml.impress.template"
            extensions="otp sti"
            globs="*.otp *.sti"
            icon_path="/++resource++mtr-icons/otp.png"
            binary="True"
            />

  <mimetype name="OpenDocument Spreadsheet"
            mimetypes="application/vnd.oasis.opendocument.spreadsheet
                       application/vnd.sun.xml.calc
                       application/vnd.stardivision.calc
                       application/x-starcalc"
            extensions="ods sxc sdc"
            globs="*.ods *.sxc *.sdc"
            icon_path="/++resource++mtr-icons/ods.png"
            binary="True"
            />

  <mimetype name="OpenDocument Spreadsheet Template"
            mimetypes="application/vnd.oasis.opendocument.spreadsheet-template
                       application/vnd.sun.xml.calc.template"
            extensions="ots stc"
            globs="*.ots *.stc"
            icon_path="/++resource++mtr-icons/ots.png"
            binary="True"
            />

  <mimetype name="OpenDocument Chart"
            mimetypes="application/vnd.oasis.opendocument.chart
                       application/vnd.stardivision.chart
                       application/x-starchart"
            extensions="odc sds"
            globs="*.odc *.sds"
            icon_path="/++resource++mtr-icons/odc.png"
            binary="True"
            />

  <mimetype name="OpenDocument Formula"
            mimetypes="application/vnd.oasis.opendocument.formula"
            extensions="odf"
            globs="*.odf"
            icon_path="/++resource++mtr-icons/oo-empty.png"
            binary="True"
            />

  <mimetype name="OpenDocument Database"
            mimetypes="application/vnd.oasis.opendocument.database"
            extensions="odb"
            globs="*.odb"
            icon_path="/++resource++mtr-icons/odb.png"
            binary="True"
            />

  <mimetype name="OpenDocument Image"
            mimetypes="application/vnd.oasis.opendocument.image"
            extensions="odi"
            globs="*.odi"
            icon_path="/++resource++mtr-icons/oo-empty.png"
            binary="True"
            />





